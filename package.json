{
  "name": "promptstash",
  "private": true,
  "scripts": {
    "dev:server": "node scripts/run-dev-server.mjs",
    "dev:electron": "pnpm --filter @promptstash/electron dev",
    "build:electron": "pnpm --filter @promptstash/electron build",
    "build:extension": "pnpm --filter @promptstash/chrome-extension build",
    "build:alfred": "node scripts/build-alfred.mjs",
    "install:alfred": "node scripts/install-alfred.mjs",
    "uninstall:alfred": "node scripts/install-alfred.mjs --uninstall",
    "dist:electron": "node scripts/dist-electron.mjs",
    "dist:extension": "node scripts/dist-extension.mjs",
    "dist:alfred": "node scripts/dist-alfred.mjs",
    "test": "pnpm -r test",
    "test:core": "pnpm --filter @promptstash/core test"
  },
  "engines": {
    "node": ">=20"
  }
}
